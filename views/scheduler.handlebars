<div class="container" id="bodycontent">
    <div class="row">
        <div class="col-md-12">
            <h4>Pending Play Dates</h4>
        {{#each playDates}}
        {{#unless accepted}}
          {{> playDates/playdate-block accepted=true}}
        {{/unless}}
        {{/each}}
        </div>
    </div>
    <hr>
    <div class="row">
        <div class="col-md-12">
            <h4>Accepted Play Dates</h4>
        {{#each playDates}}
        {{#if accepted}}
          {{> playDates/playdate-block accepted=false}}
        {{/if}}
        {{/each}}
        </div>
    </div>
</div>