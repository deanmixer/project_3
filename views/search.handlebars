<div class="container" id="bodycontent">
  <div class="row">
    <div class="col-md-12">
      {{#each user}}
        {{!-- {{#unless {{email}} == }} --}}
        <div class="card" style="max-width: 750px; margin: auto;">
          <div class="card-header">
              <div class="row">
                <div class="col-md-8 col-sm-12">
                  <h4>{{firstName}} and {{puppyName}}</h4>
                </div>
                <div class="col-md-4 col-sm-12">
                  <span><button class="btn btn-warning float-right" user-id="{{email}}">Invite for play date!</button></span>
                </div>
            </div>
          </div>
            <div class="card-body">
              <div class="row">
                <div class="col-md-4 col-sm-12">
                  <img height ="175" src={{imgUrl}}>
                </div>
                <div class="col-md-8 col-sm-12">
                  <p><b>Zip Code:</b> {{zipCode}}</p>
                  <p><b>Breed:</b> {{puppyBreed}}</p>
                  <p><b>Personality:</b> {{puppyPersonality}}</p>
                  <p><b>Dog Age:</b> {{puppyAge}}</p>
                  <p><b>Dog Weight:</b> {{puppyWeight}}</p>
                </div>
              </div>
            </div>
        </div>
            <hr>
         
      {{/each}}
    </div> 
  </div>
</div> 
